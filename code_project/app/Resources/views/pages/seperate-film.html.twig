{% extends 'base.html.twig' %}
{% block title %}De filmschuur - Film{% endblock %}
{% block body %}
<body class="lightbody">
<div class="container-fluid">
    <div class="row">
        <div class="video">
            <iframe src="{{ info[0].trailer_link }}"></iframe>
        </div>
    </div>
    <div class="row mt-5">
        <div class="col-10 offset-1 movie-details">
            <div class="row">
                <div class="col-4 offset-1 mt-3">
                    <h1>The Godfather</h1>
                    <div class="icons mt-2">
                        <img src="{{ asset('img/kijkwijzer-16.svg') }}" alt="">
                        <img src="{{ asset('img/kijkwijzer-16.svg') }}" alt="">
                        <img src="{{ asset('img/kijkwijzer-16.svg') }}" alt="">
                        <img src="{{ asset('img/kijkwijzer-16.svg') }}" alt="">
                    </div>
                    <div class="info mt-4">
                        <p>Genre: drama, misdaad</p>
                        <p>Duur: 175minuten</p>
                        <p>Release: 1972</p>
                        <p>Taal: Engels</p>
                        <p>Ondertitels: Nederlands</p>
                    </div>
                </div>
                <div class="col-5 offset-1">
                    <div class="bestellen mt-3">

                        <form action="">
                            <div class="row">
                                <div class="col-6">
                                    <label for="">Kies Datum:</label><select class="mdb-select btn-lg md-form colorful-select dropdown-primary">
                                        <option value="">12-03-2019 00:00</option>
                                        <option value="">13-03-2019 00:00</option>
                                        <option value="">14-03-2019 00:00</option>
                                        <option value="">15-03-2019 00:00</option>
                                        <option value="">16-03-2019 00:00</option>
                                        <option value="">17-03-2019 00:00</option>
                                        <option value="">18-03-2019 00:00</option>
                                    </select>
                                </div>
                        <div class="col-2 offset-3 clearfix">
                        <button type="button" class="btn order-button btn-lg">Bestellen</button>
                        </div></div>
                        </form>

                    </div>
                    <div class="inhoud mt-5">
                        <h1>Korte inhoud:</h1>
                        <p class="sans-serif">
                            De eer van de familie gaat boven alles, vooral als er een aanslag op de vader is gepleegd. Bloed moet vloeien!
                        </p>
                    </div>
                    <div class="directors head-img mt-5">
                        <h1>Regisseur(s):</h1>
                        <div class="row">
                            <div class="col-4">
                                <img src="data:image/jpeg;base64," alt="">
                            </div>
                            <div class="col-4">
                                <p>Francis Ford Coppola</p>
                            </div>
                        </div>
                    </div>
                    <div class="actors head-img mt-5 mb-2">
                        <h1>Acteurs:</h1>
                        <div class="row mt-2">
                            <div class="col-8">
                                <p>{{ info[0].actorsList }}</p>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</body>
{% endblock %}